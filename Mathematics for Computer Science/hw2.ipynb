        "#сптсок суміжності\n",
        "list_people = {'Анна': ['Богдан','Віктор','Ганна'],\n",
        "    'Богдан': ['Анна','Віктор','Дмитро'],\n",
        "    'Віктор': ['Анна','Богдан','Ганна','Дмитро'],\n",
        "    'Ганна': ['Анна','Віктор','Євген'],\n",
        "    'Дмитро': ['Богдан','Віктор','Євген'],\n",
        "    'Євген': ['Ганна','Дмитро']}\n",
        "\n",
        "vertices = list(list_people.keys())\n",
        "\n",
        "#матриця суміжності\n",
        "matrix = [[1 if v2 in list_people[v1] else 0 for v2 in vertices] for v1 in vertices]\n",
        "\n",
        "print('Матриця суміжності:')\n",
        "print('', * vertices)\n",
        "\n",
        "for v, row in zip(vertices, matrix):\n",
