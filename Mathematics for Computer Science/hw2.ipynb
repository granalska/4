        "count_full_access = 0\n",
        "found_premium_without_base = False\n",
        "\n",
        "#створюємо 32 варіанти\n",
        "all_combinations = list(itertools.product([True, False], repeat=5))\n",
        "\n",
        "for i, combination in enumerate(all_combinations):\n",
        "    is_employee = combination[0]\n",
