        "#список ребер\n",
        "ribs = []\n",
        "for v in list_people:\n",
        "    for u in list_people[v]:\n",
        "        if (u, v) not in ribs:\n",
        "            ribs.append((v, u))\n",
        "\n",
        "#степені вершин\n",
        "tops = {v: len(list_people[v])for v in list_people}\n",
        "print('\\nСтепені вершин:')\n",
        "for v, t in tops.items():\n",
        "    print(f'{v} - {t}')\n",
        "\n",
        "most = max(tops, key=tops.get)\n",
        "least = min(tops, key=tops.get)\n",
        "\n",
        "print('\\nНайбільш комунікабельний:', most, '-', tops[most])\n",
